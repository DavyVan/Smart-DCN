    param {
        lr_mult: 1
        decay_mult: 1
    }
    param {
        lr_mult: 2
        decay_mult: 0
    }
    multi_output_param {
        num_output: 1
        weight_filler {
            type: "gaussian"
            std: 0.005
        }
        bias_filler {
            type: "constant"
            value: 0.1
        }

        full_topo_file: "/home/fanquan/Desktop/output_full_topo4.txt"

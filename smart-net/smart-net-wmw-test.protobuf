name: "smart-net-test"
layer {
  name: "data"
  type: "Input"
  top: "demand"
  input_param {
    shape: {dim: 1 dim: 1 dim: 8 dim: 8}
  }
}

layer {
  name: "fc1"
  type: "InnerProduct"
  bottom: "demand"
  top: "fc1"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 1000
    weight_filler {
      type: "gaussian"
      std: 0.005
    }
    bias_filler {
      type: "constant"
      value: 0.1
    }
  }
}


layer{
	name: "relu_fc1"
	type: "ReLU"
	bottom: "fc1"
	top: "fc1"
}

layer {
  name: "dropfc1"
  type: "Dropout"
  bottom: "fc1"
  top: "fc1"
  include {
      phase: TRAIN
  }
  dropout_param{
    dropout_ratio:0.3
  }

}



layer {
  name: "fc2"
  type: "InnerProduct"
  bottom: "fc1"

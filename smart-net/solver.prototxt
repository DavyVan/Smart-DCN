# The train/test net protocol buffer definition
#net: "smart-net-without-comments.protobuf"
net: "smart-net-wmw.protobuf"
test_iter:10
test_interval:250
base_lr: 5e-5
momentum: 0.9
weight_decay: 0.004
lr_policy: "step"
gamma: 0.9
stepsize:20000
display: 250
max_iter: 1000000000
# solver mode: CPU or GPU
solver_mode: GPU
